{
  "name": "Digital Wallet application",
  "version": "1.0.0",
  "description": "It is a Digital wallet backend application which allows a customer to create and view a wallet, apply some transcations (debit and withdrawls) and view a list of all transcations for a specific wallet.",
  "main": "index.js",
  "scripts": {
    "start": "node ./src/index.js",
    "dev": "env-cmd -f ./config/dev.env nodemon DEBUG=express:*  ./src/index.js",
    "test": "env-cmd -f ./config/test.env jest --watchAll --runInBand"
  },
  "keywords": [
    "Digital-Wallet",
    "NodeJS",
    "Express",
    "JavaScript",
    "MongoDB"
  ],
  "author": "Sachin Tuli",
  "license": "ISC",
  "jest": {
    "testEnvironment": "node"
  },
  "dependencies": {
    "@types/express": "^4.17.14",
    "@types/mongodb": "^4.0.7",
    "@types/mongoose": "^5.11.97",
    "dotenv": "^16.0.3",
    "express": "^4.18.1",
    "express-validator": "^6.14.3",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^4.9.1",
    "mongoose": "^6.8.1",
    "short-unique-id": "^4.4.4",
    "uuid": "^9.0.0",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "@types/jest": "^29.0.3",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "env-cmd": "^10.1.0",
    "eslint": "^8.24.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.3.0",
    "eslint-plugin-promise": "^6.0.1",
    "jest": "^29.0.3",
    "nodemon": "^2.0.19",
    "supertest": "^6.2.4"
  }
}